<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] -</title>

    <environment names="Development">

        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
        <link href="~/lib/kendo-ui/styles/kendo.common.min.css" rel="stylesheet" />
        <link href="~/lib/kendo-ui/styles/kendo.common-material.min.css" rel="stylesheet" />
        <link href="~/lib/kendo-ui/styles/kendo.material.min.css" rel="stylesheet" />
        <link href="~/css/inspinia.css" rel="stylesheet" />
        <link rel="stylesheet" href="~/css/site.css" />
        @RenderSection("styles", required: false)
    </environment>


</head>
<body class="md-skin">
    <div id="wrapper">
        @RenderSection("sidebar", required: false)
        <div id="page-wrapper" class="gray-bg">
            @RenderBody()
        </div>
    </div>


    <environment names="Development">
        <!--Core libraries look for a way to bundle these together-->
        <script type="text/javascript" src="~/lib/lodash/dist/lodash.min.js"></script>
        <script type="text/javascript" src="~/lib/jquery/dist/jquery.min.js"></script>
        <script type="text/javascript" src="~/lib/jquery-ui/jquery-ui.min.js"></script>
        <script type="text/javascript" src="~/lib/jqueryui-touch-punch/jquery.ui.touch-punch.min.js"></script>
        <script type="text/javascript" src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>

        <!--Inspinia starts here metisMenu + slimscroll are pre-reqs to inspinia-->

        <script src="~/lib/metisMenu/dist/metisMenu.min.js"></script>
        <script src="~/lib/jquery-slimscroll/jquery.slimscroll.min.js"></script>
        <script type="text/javascript" src="~/lib/inspinia/inspinia.js"></script>
        <!--Inspinia ends here 
        <!--Core libraries end here-->
        <!--Angular 1x + Kendo-->
        <script type="text/javascript" src="~/lib/angular/angular.min.js"></script>
        <script type="text/javascript" src="~/lib/kendo-ui/js/kendo.all.min.js"></script>
        <script type="text/javascript" src="~/lib/kendo-ui/js/kendo.aspnetmvc.min.js"></script>
        <script type="text/javascript" src="~/lib/kendo-ui/js/kendo.angular.min.js"></script>

        @RenderSection("scripts", required: false)
    </environment>

  
</body>
</html>
